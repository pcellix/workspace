
public class Ducks implements Comparable<Ducks> {
	private int weight;
	
	public Ducks(int weight) {
		this.weight = weight;
	}

	@Override
	public int compareTo(Ducks otherDuck) {
		int value = 0;
		if(this.weight < otherDuck.weight) {
		    value = -1;
	    } else if(this.weight == otherDuck.weight) {
			value =  0;
	    } else if(this.weight > otherDuck.weight) {
			value = 1;
	    }
		
		return value;
	}

}
