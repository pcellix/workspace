import java.util.Arrays;


public class Main {
		
	public static void main(String[] args) {
		Ducks[] ducks = {new Ducks(12), new Ducks(32), new Ducks(1)};
		Arrays.sort(ducks);
		
		for(int i = 0; i < ducks.length; i++) {
			System.out.println("Ducks weight" + ducks[i].getWeight());
		}
		
		
		
	}

}
