document.observe("dom:loaded", function() {
	$('ja').setStyle({
		backgroundColor: '#900',
		fontSize: '12px'
	});
	$$('a:active').each(
			   function (e) {
			      e.setStyle({font-color:'green';}); 
			   } 
			);
	$$('a').each(
			   function (e) {
			      e.setStyle({border:'1px solid green'}); 
			   } 
			);
	
});
